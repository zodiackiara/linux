SYSTEM = scf #注释
PREC = Accurate  #精度一般为Accurate
ALGO = Fast      #常用Fast自洽算法
NELM = 100       #电子步最大步数
EDIFF = 1E-6     #电子步收敛标准
EDIFFG = -0.01   #离子步收敛标准
POTIM = 0.1      #离子步步长
NSW = 0          #不进行结构优化-scf
ISMEAR = 0       #半导体和绝缘体ISMEAR=0-relax
SIGMA = 0.01     #半导体和绝缘体SIGMA=0.01-relax
IBRION = -1      #不进行结构优化-scf
ISIF = 0         #不进行结构优化-scf
ISTART = 0       #不读取WAVECAR-relax
LWAVE = True     #生成WAVECAR-scf
LCHARG = True    #生成CHGCAR-scf
LREAL = False    #原子数小于20时LREAL=False，否则LREAL=Auto!
ENCUT = 520      #平面波的截断能ENCUT=1.3*ENMAX!
NPAR = 64        #NPAR=节点数!